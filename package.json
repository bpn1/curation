  "scripts": {
    "prebuild": "rimraf dist",
    "build:client": "rimraf dist && NODE_ENV=production webpack --config ./webpack.config.babel.js --progress --profile --colors",
    "prebuild:dev": "rimraf dist",
    "build:client:dev": "NODE_ENV=development webpack --config ./webpack.config.babel.js",
    "start": "npm run build:client && npm run server",
    "dev": "webpack-dev-server --env development",
    "server": "NODE_ENV=production node src/server.js",
    "server:dev": "NODE_ENV=development node src/server.js",
    "debug": "node-nightly --inspect --debug-brk node_modules/.bin/webpack --env debug",
    "debug:dev": "npm run debug -- --env development",
    "debug:prod": "npm run debug -- --env production",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "yarn run test -- --watch",
    "test:update": "yarn run test -- -u",
    "validate": "npm-run-all --parallel lint build test",
    "lint:code": "eslint src/**/*.js src/**/*.jsx webpack.config.babel.js --cache -f table",
    "lint:styles": "stylelint src/**/*.css",
    "lint:autofix": "yarn run lint -- --fix"
  },
  "jest": {
    "verbose": true,
    "rootDir": "src",
    "setupFiles": [
      "../jest.config.js"
    ],
    "moduleNameMapper": [
      [
        "^.+\\.(css)$",
        "identity-obj-proxy"
      ]
    ],
    "snapshotSerializers": [
      "../node_modules/enzyme-to-json/serializer"
    ]
  }
}
